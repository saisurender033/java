class remo_static 
{
    static int a=10;
    static int b;
    static void meth(int x)
    {
        System.out.println("x="+x);
        System.out.println("a="+a);
        System.out.println("b="+b);
    }
    static
    {
        System.out.println("static bock initilized");
        b=a*10;
    }
}
class Main //remo_staticdemo
{
    public static void main (String args[])
    {
        remo_static.meth(30);
    }
}
